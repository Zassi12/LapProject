@model LAP.Web.Models.ChartModel


@{
    ViewBag.Title = Model.Firma;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Chart</h2>

@{
    var myChart = new Chart(width: 600, height: 400)
        .AddTitle("Umsatz")
        .AddSeries(
            name: "Firma",
            xValue: new[] { Model.Firma},
            yValues: new[] { Model.Ausgaben })
        .Write();
}



